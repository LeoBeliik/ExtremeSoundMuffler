{
  "schemaVersion": 1,
  "id": "extremesoundmuffler",
  "version": "${version}",
  "name": "Extreme sound muffler",
  "description": "This mod allows you to muffle (almost) any sound selectively, allowing you to choose the volume of the sound you want between mute and 90%. This mod also works Clientside only.",
  "authors": [
    "LeoBeliik"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/extreme-sound-muffler",
    "sources": "https://github.com/LeoBeliik/ExtremeSoundMuffler",
    "issues": "https://github.com/LeoBeliik/ExtremeSoundMuffler/issues"
  },
  "license": "LGPL-3.0",
  "icon": "esm_logo.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.leobeliik.extremesoundmuffler.SoundMufflerFabric"
    ]
  },
  "mixins": [
    "extremesoundmuffler.mixins.json"
  ],
  "custom": {
    "modmenu:clientsideOnly": true
  },
  "depends": {
    "fabricloader": ">=0.12",
    "fabric": ">=0.45",
    "minecraft": ">=${minecraft_version_range}",
    "java": ">=17"
  }
}
  